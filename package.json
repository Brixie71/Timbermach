{
  "name": "timbermach",
  "files": [
    "dist"
  ],
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "dev:react": "vite --host",
    "dev:laravel": "cd C:/xampp/htdocs/TIMBER && php artisan serve",
    "dev:python": "cd python-backend && python app.py",
    "dev:electron": "electron electron/main.cjs",
    "dev:fullstack": "concurrently -n \"REACT,LARAVEL,PYTHON\" -c \"blue,yellow,green\" \"npm run dev:react\" \"npm run dev:laravel\" \"npm run dev:python\"",
    "dev:all": "concurrently -n \"REACT,LARAVEL,PYTHON,ELECTRON\" -c \"blue,yellow,green,magenta\" \"npm run dev:react\" \"npm run dev:laravel\" \"npm run dev:python\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "start": "npm run dev:fullstack",
    "start:electron": "npm run dev:all",
    "electron": "electron electron/main.cjs",
    "electron:dev": "concurrently -n \"REACT,LARAVEL,PYTHON\" -c \"blue,yellow,green\" \"npm run dev:react\" \"npm run dev:laravel\" \"npm run dev:python\" && wait-on http://localhost:5173 && electron electron/main.cjs",
    "package": "electron-builder"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@serialport/parser-readline": "^13.0.0",
    "axios": "^1.8.4",
    "chart.js": "^4.5.1",
    "d3": "^7.9.0",
    "electron": "^39.2.4",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.486.0",
    "mathjs": "^14.4.0",
    "opencv.js": "^1.2.1",
    "papaparse": "^5.4.1",
    "plotly.js": "^2.29.1",
    "react": "^19.1.0",
    "react-accessible-accordion": "^5.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-countup": "^6.5.3",
    "react-crud": "^0.0.1",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-outside-click-handler": "^1.3.0",
    "react-pro-sidebar": "^1.1.0",
    "react-router-dom": "^7.4.1",
    "react-simple-keyboard": "^3.8.57",
    "serialport": "^13.0.0",
    "simple-keyboard": "^3.8.44",
